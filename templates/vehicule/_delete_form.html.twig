<form method="post" action="{{ path('vehicule_delete', {'idvehicule': vehicule.idvehicule}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ vehicule.idvehicule) }}">
    <button class="btn">Delete</button>
</form>
