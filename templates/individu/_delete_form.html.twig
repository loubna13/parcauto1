<form method="post" action="{{ path('individu_delete', {'idindividu': individu.idindividu}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ individu.idindividu) }}">
    <button class="btn">Supprimer</button>
</form>
